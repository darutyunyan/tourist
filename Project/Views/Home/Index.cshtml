@section Content {
    <div class="container-fluid pl-0 pr-0">
        <div id="map"></div>
    </div>
}

@section scripts{
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: -34.397, lng: 150.644 },
                zoom: 8
            });
        }
        initMap();
    </script>
}